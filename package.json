{
	"name": "@magic-mustard/sqlsync",
	"version": "0.0.1",
	"description": "SQLSync simplifies database schema evolution by allowing a declarative approach to table management",
	"main": "dist/index.js",
	"bin": {
		"sqlsync": "dist/cli.js"
	},
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"build": "tsc",
		"start": "node dist/cli.js",
		"dev": "ts-node src/cli.ts",
		"test": "jest",
		"format": "prettier --write \"**/*.{ts,js,json,md}\"",
		"prepublishOnly": "pnpm run build"
	},
	"keywords": ["sqlsync", "postgresql", "database", "migrations", "declarative", "schema", "sync"],
	"author": "Tim | magicMustard",
	"license": "MIT",
	"type": "commonjs",
	"dependencies": {
		"@types/node-sql-parser": "^1.0.0",
		"chalk": "^4.1.2",
		"commander": "^12.1.0",
		"inquirer": "^12.5.0",
		"js-yaml": "^4.1.0",
		"module-alias": "^2.2.3",
		"node-sql-parser": "^5.3.8",
		"typescript": "^5.4.5",
		"yaml": "^2.3.4"
	},
	"devDependencies": {
		"@types/inquirer": "^9.0.7",
		"@types/jest": "^29.5.12",
		"@types/js-yaml": "^4.0.9",
		"@types/module-alias": "^2.0.4",
		"@types/node": "^20.12.12",
		"jest": "^29.7.0",
		"prettier": "^3.5.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2"
	}
}
